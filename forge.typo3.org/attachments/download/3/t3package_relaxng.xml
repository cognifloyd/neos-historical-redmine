<element name="T3Package" xmlns="http://relaxng.org/ns/structure/1.0">
	<element name="key">
		<text/>
	</element>
	<element name="name">
		<text/>
	</element>
	<element name="description">
		<text/>
	</element>
	<element name="version">
		<text/>
	</element>
	<element name="status">
		<text/>
	</element>
	<optional>
		<element name="requires">
			<interleave>
				<zeroOrMore>
					<element name="package">
						<text/>
					</element>
				</zeroOrMore>
				<zeroOrMore>
					<element name="component">
						<text/>
					</element>
				</zeroOrMore>
			</interleave>
		</element>
	</optional>
	<element name="authors">
		<oneOrMore>
			<element name="author">
				<element name="name">
					<text/>
				</element>
				<element name="email">
					<text/>
				</element>
				<element name="t3orguser">
					<text/>
				</element>
			</element>
		</oneOrMore>
	</element>
</element>

		<developer email="other@example.com" website="http://www.example.com" type="co-developer">Mr. Y</developer>
		<developer email="john@example.com" website="http://www.example.com" type="contributor">John Doe</developer>
		<developer email="company@example.com" website="http://www.example.com" type="company">ACME Inc.</developer>
